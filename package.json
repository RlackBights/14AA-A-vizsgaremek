{
  "name": "learnthebasics",
  "version": "1.0.0",
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "build": {
    "appId": "com.learnthebasics.app",
    "win": {
      "icon": "build/logo.ico"
    }
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "mysql": "^2.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "react-scripts build && electron ./public/electron.js --dev",
    "pack": "react-scripts build && electron-builder --dir && xcopy /s .\\build .\\dist\\win-unpacked\\resources"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "A game that teaches people the basics of webdev and pc building",
  "main": "electron-main.js",
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@electron-forge/cli": "^7.2.0",
    "@iconify/react": "^4.1.1",
    "electron": "^29.1.2",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^3.0.1"
  },
  "author": "LtB Team",
  "license": "ISC"
}
